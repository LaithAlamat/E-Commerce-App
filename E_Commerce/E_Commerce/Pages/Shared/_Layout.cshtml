<!DOCTYPE html>
@using E_Commerce.Models
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
</head>
<body>
    <header style="display:flex; ">
        <img src="data:image/png;base64," alt="HomeLogo" class="logo" style="width: 100px; height:100px">
        <nav class="navbar" style="display: flex; justify-content: space-evenly; align-items: center;">
            <a asp-controller="Home" asp-action="Index" class="headertag">Home</a>
            <a asp-controller="Category" asp-action="List" class="headertag">Categories</a>
            <a asp-controller="Product" asp-action="List" class="headertag">Products</a>

        </nav>
    </header>
    @RenderBody()
</body>
</html>
